				2: longsprite_e2_series_pantof_red;	    // Base for LongSprite
				3: longsprite_e2_series_pantof_mask;       // Masks for LongSprite
				 // Pantos
                4: panto_midsprite_e2_f;
                5: panto_longsprite_e2_f;
			}

