		switch(FEAT_TRAINS, SELF, sw_e2_series_pantob_bck_longsprite, NextPartSame() ) { 1: sw_e2_series_pantob_bck_stack; dummy_sprites; }

		switch (FEAT_TRAINS, SELF, sw_e2_series_pantob, position_in_consist_from_end % 3 ) {				
