		ss_200_series_cabwp;				
	}

switch (FEAT_TRAINS, PARENT, sw_200_series_front_reversecheck, vehicle_is_reversed) {				
		0:  ss_200_series_cab;
		sw_200_series_front_oddevencheck;				
	}

switch (FEAT_TRAINS, SELF, sw_200_series_front, position_in_consist) {				
		1:  sw_200_series_front_reversecheck;
		sw_200_series_back;				
	}

// GRAPHICS
switch (FEAT_TRAINS, SELF, sw_200_series_graphics, position_in_consist % 3) {				
		1:  sw_200_series_front; 				
		dummy_sprites;
	}

switch (FEAT_TRAINS, SELF, sw_200_series_articulated_part, extra_callback_info1) {
		1 .. 2: return series_200;									
		return CB_RESULT_NO_MORE_ARTICULATED_PARTS;
	}

/*---Coupling Switch---*/
switch(FEAT_TRAINS, SELF, sw_200_series_attach_vehid, [vehicle_type_id == mu_car_full]) {
 		0: return string(ONLY_ADD_MUCAR);
}

/*---Length Check---*/
switch (FEAT_TRAINS, SELF, sw_200_series_stop_start, num_vehs_in_consist < 12) {				
		1:  return string(STR_LENGTH_ERROR); 				
		return CB_RESULT_NO_TEXT; 
	}

